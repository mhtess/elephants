@article{Levy2008,
abstract = {This paper investigates the role of resource allocation as a source of processing difficulty in human sentence comprehension. The paper proposes a simple information-theoretic charac-terization of processing difficulty as the work incurred by resource reallocation during parallel, incremental, probabilistic disambiguation in sentence comprehension, and demonstrates its equivalence to the theory of Hale [Hale, J. (2001). A probabilistic Earley parser as a psycho-linguistic model. In Proceedings of NAACL (Vol. 2, pp. 159â€“166)], in which the difficulty of a word is proportional to its surprisal (its negative log-probability) in the context within which it appears. This proposal subsumes and clarifies findings that high-constraint contexts can facil-itate lexical processing, and connects these findings to well-known models of parallel con-straint-based comprehension. In addition, the theory leads to a number of specific predictions about the role of expectation in syntactic comprehension, including the reversal of locality-based difficulty patterns in syntactically constrained contexts, and conditions under which increased ambiguity facilitates processing. The paper examines a range of established results bearing on these predictions, and shows that they are largely consistent with the sur-prisal theory.},
author = {Levy, Roger},
doi = {10.1016/j.cognition.2007.05.006},
file = {:Users/mht/Library/Application Support/Mendeley Desktop/Downloaded/Levy - 2008 - Expectation-based syntactic comprehension.pdf:pdf},
journal = {Cognition},
keywords = {Frequency,Information theory,Parsing,Prediction,Sentence processing,Syntactic complexity,Syntax,Word order},
mendeley-groups = {To read,language},
pages = {1126--1177},
title = {{Expectation-based syntactic comprehension}},
volume = {106},
year = {2008}
}

